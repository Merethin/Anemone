{
  "name": "anemone",
  "version": "0.1.2",
  "description": "N-Day userscript for JEFF",
  "main": "src/main.ts",
  "scripts": {
    "prebuild": "mkdir -p build && node -p \"'export const VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > build/version.ts",
    "build": "esbuild src/main.ts --bundle --outfile=build/out.js --format=iife && cat header.txt build/out.js >anemone.user.js"
  },
  "author": "Merethin",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@types/mousetrap": "1.6.15",
    "@types/tampermonkey": "5.0.4",
    "esbuild": "0.25.5",
    "mousetrap": "1.6.5"
  },
  "dependencies": {
    "idb-keyval": "^6.2.2"
  }
}
